{
    "layout": {
        "split": true,
        "rows": 3,
        "columns": 5,
        "thumbs": 2
    },
    "layers": {
        "qwerty": {
            "left": [
                ["q", "w", "e", "r", "t"],
                ["a", "s", "d", "f", "g"],
                ["z", "x", "c", "v", "b"]
            ],
            "right": [
                ["y", "u", "i", "o", "p"],
                ["h", "j", "k", "l", ";"],
                ["n", "m", ",", ".", "/"]
            ],
            "thumbs": {
                "left":  ["nav", "shift"],
                "right": ["space", "sym"] 
            } 
        },
        "sym": {
            "left": [
                ["!", "@", "#", "$", "%"],
                ["&", "*", "_", "'", "\""],
                ["|", "+", "-", "=", "\\"]
            ],
            "right": [
                ["^", "~","(", ")", "bksp" ],
                ["`", "´", "{", "}", "del" ],
                [null, "¨", "[", "]", "enter"]
            ],
            "thumbs": {
                "left": [ "nav", "shift"],
                "right": [ "space", "hold::sym"]
            } 
        },
        "nav": {
            "left": [
                ["esc", "swap win", "search", "repeat", "tab"],
                ["shift", "ctrl", "alt", "cmd", "cancel"],
                ["undo", "cut", "copy", "paste", null]
            ],
            "right": [
                ["pgup", "home", "↑", "end", "bksp"],
                ["pgdn", "←", "↓", "→", "del"],
                [null, "æ", "ø", "å", "enter"]
            ],
            "thumbs": {
                "left": [ "hold::nav", "shift"],
                "right": [ "space", "sym"]
            } 
        },
        "num": {
            "left": [
                [null, "7", "8", "9", "tab"],
                [",", "4", "5", "6", null],
                [".", "1", "2", "3", "0"]
            ],
            "right": [
                ["f1", "f2", "f3", "f4", "bksp"],
                ["f5", "f6", "f7", "f8", "del"],
                ["f9", "f10", "f11", "f12", "enter"]
            ],
            "thumbs": {
                "left": [ "hold::nav", "shift"],
                "right": [ "space", "hold::sym"]
            } 
        }
    }
}

