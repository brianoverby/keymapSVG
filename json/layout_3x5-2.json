{
    "layout": {
        "split": true,
        "rows": 3,
        "columns": 5,
        "thumbs": 2
    },
    "layers": {
        "qwerty": {
            "left": [
                ["q", "w", "e", "r", "t"],
                ["a", "s", "d", "f", "g"],
                ["z", "x", "c", "v", "b"]
            ],
            "right": [
                ["y", "u", "i", "o", "p"],
                ["h", "j", "k", "l", ";"],
                ["n", "m", ",", ".", "/"]
            ],
            "thumbs": {
                "left":  ["nav", "shift"],
                "right": ["space", "sym"] 
            } 
        },
        "sym": {
            "left": [
                ["!", "@", "#", "$", "%"],
                ["&", "*", "_", "'", "\""],
                ["|", "+", "-", "=", "\\"]
            ],
            "right": [
                ["(", ")", "^", null, "~"],
                ["{", "}", "¨", "´", "`"],
                ["[", "]", "æ", "ø", "å"]
            ],
            "thumbs": {
                "left": [ "num", "shift"],
                "right": [ "space", "hold::sym"]
            } 
        },
        "nav": {
            "left": [
                ["tab", null, null, null, null],
                ["shift", "ctrl", "alt", "cmd", null],
                ["esc", null, null, null, null]
            ],
            "right": [
                ["pgup", "home", "up", "end", "bksp"],
                ["pgdn", "left", "down", "right", "del"],
                [null, null, null, null, "enter"]
            ],
            "thumbs": {
                "left": [ "hold::nav", "shift"],
                "right": [ "space", "num"]
            } 
        },
        "num": {
            "left": [
                [null, "7", "8", "9", ","],
                [null, "4", "5", "6", "."],
                [null, "1", "2", "3", "0"]
            ],
            "right": [
                ["f1", "f2", "f3", "f4", "f5"],
                ["f6", "f7", "f8", "f9", "f10"],
                ["f11", "f12", null, null, null]
            ],
            "thumbs": {
                "left": [ "hold::num", "shift"],
                "right": [ "space", "hold::num"]
            } 
        }
    }
}

