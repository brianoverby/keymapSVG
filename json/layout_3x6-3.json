{
    "layout": {
        "split": true,
        "rows": 3,
        "columns": 6,
        "thumbs": 3,
        "thumb_offset": 1
    },
    "layers": {
        "qwerty": {
            "left": [
                ["esc","q", "w", "e", "r", "t"], 
                ["ctrl","a", "s", "d", "f", "g"],
                ["shift","z", "x", "c", "v", "b"]
            ],
            "right": [
                ["y", "u", "i", "o", "p", "bksp"],
                ["h", "j", "k", "l", ";", "'"],
                ["n", "m", ",", ".", "/", "tab"]
            ],
            "thumbs": {
                "left":  ["cmd", "nav", "space"],
                "right": ["enter", "sym", "opt"] 
            } 
        },
        "nav": {
            "left": [
                ["tab","lock","swap win", "search", "vol down", "vol up"],
                ["ctrl","f1", "f2", "f3", "f4", "f5"],
                ["shift","f6", "f7", "f8", "f9", "f10"]
            ],
            "right": [
                ["pgup", "home", "up", "end", null, "bksp"],
                ["pgdn", "left", "down", "right", null, null],
                ["f11", "f12", "æ", "ø", "å", "tab"]
            ],
            "thumbs": {
                "left":  ["cmd", "hold::nav", "space"],
                "right": ["enter", "sym", "opt"]  
            } 
        },
        "sym": {
            "left": [
                ["tab", "!", "@", "#", "$", "%"],
                ["ctrl", "1", "2", "3", "4", "5"],
                ["shift", "6", "7", "8", "9", "0"]
            ],
            "right": [
                ["^", "&", "*", "(", ")", "\\"],
                ["-", "=", "`", "{", "}", "|"],
                ["_", "=", "`", "[", "]", "tab"]
            ],
            "thumbs": {
                "left":  ["cmd", "nav", "space"],
                "right": ["enter", "hold::sym", "opt"] 
            } 
        }
    }
}

