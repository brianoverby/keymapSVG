{
    "layout": {
        "split": true,
        "rows": 3,
        "columns": 5,
        "thumbs": 3,
        "thumb_offset": 1
    },
    "layers": {
        "qwerty": {
            "left": [
                ["q", "w", "e", "r", "t"],
                ["a", "s", "d", "f", "g"],
                ["z", "x", "c", "v", "b"]
            ],
            "right": [
                ["y", "u", "i", "o", "p"],
                ["h", "j", "k", "l", ";"],
                ["n", "m", ",", ".", "/"]
            ],
            "thumbs": {
                "left":  ["cmd","sym", "shift"],
                "right": ["space", "nav","ctrl"] 
            } 
        },
        "sym": {
            "left": [
                ["!", "@", "#", "$", "%"],
                ["&", "*", "_", "'", "\""],
                ["|", "+", "-", "=", "\\"]
            ],
            "right": [
                ["^", "~", "(", ")", "bksp"],
                ["`", "´","{", "}", "del"],
                [null, "¨","[", "]", "enter"]
            ],
            "thumbs": {
                "left": [ "cmd","hold::sym", "shift"],
                "right": [ "space", "nav","ctrl"]
            } 
        },
        "nav": {
            "left": [
                ["esc", "7", "8", "9", ","],
                ["tab", "4", "5", "6", "."],
                [null, "1", "2", "3", "0"]
            ],
            "right": [
                ["pgup", "home", "↑", "end", "bksp"],
                ["pgdn", "←", "↓", "→", "del"],
                [null, "æ", "ø", "å", "enter"]
            ],
            "thumbs": {
                "left":  ["cmd","sym", "shift"],
                "right": ["space", "hold::nav","ctrl"] 
            } 
        },
        "fun": {
            "left": [
                ["f1", "f2", "f3", "f4", null],
                ["f5", "f6", "f7", "f8", null],
                ["f9", "f10", "f11", "f12", null]
            ],
            "right": [
                [null, null, null, null, null],
                ["ctrl", "opt", "shift", null, null],
                [null, null, null, null, null]
            ],
            "thumbs": {
                "left":  ["cmd","hold::sym", "shift"],
                "right": ["space", "hold::nav","ctrl"] 
            } 
        }
    }
}

